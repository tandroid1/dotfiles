[alias]
  st = status
  cm = commit
  br = branch -av
  brd = branch -d
  co = checkout
  cod = checkout develop
  com = checkout master
  cr = clone --recursive
  pa = push --all
  pt = push --tags
  pat = !git pa && git pt
  pu = push -u
  pua = push -u --all
  aa = add -A :/
  fi = flow init -d
  fs = flow feature start
  ff = flow feature finish
  rs = flow release start
  rf = flow release finish
  hs = flow hotfix start
  hf = flow hotfix finish
  df = diff
  lg = log -p
  l1 = log --max-count=1
  l2 = log --max-count=2
  l3 = log --max-count=3
  l4 = log --max-count=4
  l5 = log --max-count=5
  lsd = log --graph --decorate --pretty=oneline --abbrev-commit --all
  lss = log --graph --decorate --pretty=oneline --abbrev-commit --all --max-count=30
  clsd = log --graph --decorate --pretty=oneline --abbrev-commit
  rl = reflog
  rb = rebase
  freebase = pull --rebase
[push]
  default = simple
[color]
  ui = true
